{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "group",
  "type": "object",
  "properties": {
    "location": {
      "type": "string",
      "pattern": "^[0-9]*$",
      "minLength": 6
    },
    "name": {
      "type": "string",
      "minLength": 1
    }
  },
  "required": [
    "location","name"
  ],
  "additionalProperties": false
}
